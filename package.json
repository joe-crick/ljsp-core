{
  "name": "ljsp-core",
  "version": "0.1.9",
  "description": "A lisp-inspired functional library for javascript programmers",
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-crick/ljsp-core"
  },
  "jsnext:main": "dist/index.js",
  "main": "dist/lib.cjs.js",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/lib.cjs.js"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "release": "pnpm run build && pnpm publish",
    "release:beta": "pnpm run build && pnpm publish --tag beta",
    "test": "jest --watch",
    "test:once": "jest",
    "lint": "eslint lib/**/*.ts tests/**/*.ts --ignore-pattern=lib/**/*.d.ts --fix --max-warnings=0",
    "prettify:all": "prettier --write \"**/*.ts*\""
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "lisp",
    "javascript",
    "functional",
    "util",
    "utils",
    "utilities",
    "functions",
    "toolkit",
    "fp",
    "functional programming",
    "clojure"
  ],
  "author": "Joseph Crick <joseph.crick@finleap.com>",
  "contributors": [
    "Dania Duany <dania.duany@finleap.com>",
    "Ashish Santikari <hi@ashishsantikari.info>",
    "Josip Brƒçina <josipbrchina@gmail.com>"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@rollup/plugin-url": "^8.0.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-loader": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "fast-check": "^4.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "rollup": "^4.46.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "sideEffects": false,
  "type": "module"
}