{
  "name": "ljsp-core",
  "version": "0.1.9",
  "description": "A lisp-inspired functional library for javascript programmers",
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-crick/ljsp-core"
  },
  "jsnext:main": "dist/index.js",
  "main": "dist/lib.cjs.js",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/lib.cjs.js"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "release": "pnpm run build && pnpm publish",
    "release:beta": "pnpm run build && pnpm publish --tag beta",
    "test": "jest --watch",
    "test:once": "jest",
    "lint": "eslint lib/**/*.ts tests/**/*.ts --ignore-pattern=lib/**/*.d.ts --fix --max-warnings=0",
    "prettify:all": "prettier --write \"**/*.ts*\""
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "lisp",
    "javascript",
    "functional",
    "util",
    "utils",
    "utilities",
    "functions",
    "toolkit",
    "fp",
    "functional programming",
    "clojure"
  ],
  "author": "Joseph Crick <joseph.crick@finleap.com>",
  "contributors": [
    "Dania Duany <dania.duany@finleap.com>",
    "Ashish Santikari <hi@ashishsantikari.info>",
    "Josip Brƒçina <josipbrchina@gmail.com>"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.23.9",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-loader": "^9.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "sideEffects": false,
  "type": "module"
}